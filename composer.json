{
    "name": "stanlemon/report-bundle",
    "type": "symfony-bundle",
    "description": "Dynamic query reporting bundle for Symfony 2",
    "keywords": ["report", "query", "dynamic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stan Lemon",
            "email": "stosh1985@gmail.com",
            "homepage": "http://stanlemon.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.2",
        "symfony/form": "~2.2",
        "symfony/yaml": "~2.1",
        "doctrine/doctrine-bundle": "*",
        "white-october/pagerfanta-bundle": "1.*",
    },
    "suggest": {
        "stanlemon/bootstrap-bundle": "Bootstrap layout"
    },
    "autoload": {
        "psr-0": { "Lemon\\ReportBundle": "" }
    },
    "target-dir": "Lemon/ReportBundle",
    "config": {
        "bin-dir": "bin"
    }
}